<Page
	x:Class="OpenBullet.DialogCustomInput"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	Title="DialogCustomInput"
	xmlns:openbullet="clr-namespace:OpenBullet">
	<StackPanel
		Height="100"
		Width="490"
		Margin="5">
		<TextBox
			x:Name="questionTextbox"
			Text="PLACEHOLDER"
			BorderThickness="0"
			Background="#00FFFFFF"
			IsReadOnly="True" />
		<TextBox
			x:Name="answerTextbox"
			KeyDown="answerTextbox_KeyDown" />
		<Button
			x:Name="acceptButton"
			Content="ACCEPT"
			Margin="0 5 0 0"
			Click="acceptButton_Click" />
	</StackPanel>
</Page>
