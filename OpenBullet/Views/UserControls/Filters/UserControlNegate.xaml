<UserControl
	x:Class="OpenBullet.Views.UserControls.Filters.UserControlNegate"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="clr-namespace:OpenBullet.Views.UserControls.Filters"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Initialized="UserControl_Initialized">
	<StackPanel
		Margin="0,2,0,0">
		<CheckBox
			Name="CheckBoxOnlyGrayscale"
			Padding="0"
			Content="OnlyGrayscale"
			Click="CheckBoxOnlyGrayscale_Click" />
		<Label
			Content="Channels" />
		<ComboBox
			Name="ChannelsComboBox"
			SelectionChanged="ChannelsComboBox_SelectionChanged" />
	</StackPanel>
</UserControl>
