<Page
	x:Class="OpenBullet.Views.Main.Configs.ConfigOtherOptions"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
	xmlns:local="clr-namespace:OpenBullet.Views.Main.Configs"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	Title="ConfigOtherOptions">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="30" />
			<RowDefinition
				Height="*" />
		</Grid.RowDefinitions>
		<StackPanel
			x:Name="topMenu"
			Orientation="Horizontal">
			<FrameworkElement.Resources>
				<ResourceDictionary>
					<Style
						x:Key="{x:Type Label}"
						TargetType="{x:Type Label}">
						<Setter
							Property="FontWeight"
							Value="Bold" />
						<Setter
							Property="Margin"
							Value="0,0,2,0" />
						<Setter
							Property="Foreground"
							Value="{DynamicResource ForegroundMain}" />
						<Setter
							Property="FontFamily"
							Value="pack://application:,,,/Fonts/#Century Gothic" />
						<Setter
							Property="FontSize"
							Value="12" />
						<Setter
							Property="VerticalAlignment"
							Value="Center" />
					</Style>
				</ResourceDictionary>
			</FrameworkElement.Resources>
			<Label
				x:Name="menuOptionGeneral"
				MouseDown="menuOptionGeneral_MouseDown">
				<DockPanel>
					<iconPacks:PackIconMaterial
						Margin="0,0,3,0"
						Kind="Earth" />
					<TextBlock>General</TextBlock>
				</DockPanel>
			</Label>
